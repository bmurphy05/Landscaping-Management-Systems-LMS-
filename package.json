{
   "name": "Landscaping-Management-Systems-V2",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "devDependencies": {
      "@types/bcryptjs": "^2.4.2",
      "@types/jest": "^24.0.18",
      "@types/node": "^8.0.29",
      "@types/uuid": "^3.4.3",
      "gql2ts": "^1.10.1",
      "jest": "^24.9.0",
      "nodemon": "^1.19.2",
      "ts-jest": "^24.1.0",
      "ts-node": "3.3.0",
      "tslint": "^5.20.0",
      "tslint-config-prettier": "^1.18.0",
      "typescript": "3.3.3333"
   },
   "dependencies": {
      "bcryptjs": "^2.4.3",
      "graphql": "^14.5.8",
      "graphql-import": "^0.7.1",
      "graphql-request": "^1.8.2",
      "graphql-tools": "^4.0.5",
      "graphql-yoga": "^1.18.3",
      "pg": "^7.3.0",
      "reflect-metadata": "^0.1.10",
      "typeorm": "0.2.19",
      "uuid": "^3.3.3"
   },
   "scripts": {
      "start": "NODE_ENV=development nodemon --exec ts-node src/index.ts",
      "test-server": "NODE_ENV=test ts-node src/index.ts",
      "test": "NODE_ENV=test jest --forceExit",
      "ci": "start-server-and-test test-server http://localhost:4000 test",
      "gen-schema-types": "gql2ts src/schema.graphql -o src/types/schema.d.ts"
   },
   "jest": {
      "transform": {
         "^.+\\.tsx?$": "ts-jest"
      },
      "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
      "moduleFileExtensions": [
         "ts",
         "tsx",
         "js",
         "jsx",
         "json",
         "node"
      ]
   }
}
